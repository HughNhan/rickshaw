{
    "rickshaw-bench-metric": {
        "type": "dict",
        "required": true,
        "schema": {
            "schema": {
                "type": "dict",
                "required": true,
                "schema": {
                    "version": {
                        "allowed": [
                            "2020.03.18"
                        ],
                        "required": true
                    }
                }
            }
        }
    },
    "periods": {
        "type": "list",
        "required": true,
        "empty": false,
        "schema": {
            "type": "dict",
            "schema": {
                "name": {
                },
                "metrics": {
                    "type": "list",
                    "schema": {
                        "type": "dict",
                        "schema": {
                            "data": {
                                "type": "list",
                                "required": true,
                                "empty": false,
                                "schema": {
                                    "type": "dict",
                                    "schema": {
                                        "value": {
                                            "type": "string",
                                            "required": true,
                                            "empty": false
                                        },
                                        "end": {
                                            "type": "integer",
                                            "required": true,
                                            "empty": false
                                        },
                                        "begin": {
                                            "type": "integer",
                                            "required": false,
                                            "empty": false
                                        }
                                    }
                                }
                            },
                            "desc": {
                                "type": "dict",
                                "schema": {
                                    "class": {
                                        "type": "string",
                                        "required": true,
                                        "empty": false
                                    },
                                    "type": {
                                        "type": "string",
                                        "required": true,
                                        "empty": false
                                    },
                                    "name-format": {
                                        "type": "string",
                                        "required": true,
                                        "empty": false
                                    },
                                    "source": {
                                        "type": "string",
                                        "required": true,
                                        "empty": false
                                    }
                                }
                            },
                            "names": {
                                "type": "dict"
                            }
                        }
                    }
                }
            }
        }
    }
}
